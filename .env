# =============================================================================
# Application Settings
# =============================================================================
NEXT_PUBLIC_NAT_WORKFLOW=NeMo Agent Toolkit
NEXT_PUBLIC_NAT_WEB_SOCKET_DEFAULT_ON=false
NEXT_PUBLIC_NAT_CHAT_HISTORY_DEFAULT_ON=false
NEXT_PUBLIC_NAT_RIGHT_MENU_OPEN=false
NEXT_PUBLIC_NAT_ENABLE_INTERMEDIATE_STEPS=true
NEXT_PUBLIC_NAT_SHOW_DATA_STREAM_DEFAULT_ON=false
NEXT_PUBLIC_NAT_ADDITIONAL_VIZ_DEFAULT_ON=false

# =============================================================================
# Backend Configuration
# =============================================================================
# REQUIRED: Your API server backend address
# Always include the protocol (http:// or https://)
NAT_BACKEND_URL="http://localhost:8000/generate"

# =============================================================================
# Proxy Configuration
# =============================================================================
PORT=3000
NEXT_INTERNAL_URL=http://localhost:3001

# Public HTTP and WebSocket paths the browser uses to reach the gateway.
HTTP_PUBLIC_PATH=/api
WS_PUBLIC_PATH=/ws

# =============================================================================
# Other Configuration
# =============================================================================
NODE_ENV=development
NAT_DEFAULT_MODEL=
NAT_MAX_FILE_SIZE_STRING=5mb
RAG_UUID=
